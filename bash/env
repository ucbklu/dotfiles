#Environment variables

#Sublime Text 2
export EDITOR='subl -w'

#Bundler -- using "bundle install --binstubs .bundle/bin"
export PATH="./.bundle/bin:$PATH"

#Brew packages
export PATH="/usr/local/bin:/usr/local/sbin:~/bin:$PATH"

#RBENV
export PATH="$HOME/.rbenv/bin:$PATH"
eval "$(rbenv init -)"

#acquia drupal
PATH=~/bin:/Applications/acquia-drupal/mysql/bin:/Applications/acquia-drupal/php/bin:/Applications/acquia-drupal/apache/bin:$PATH

